{
    "extends": ["config:base"],
    "regexManagers": [
      {
        "fileMatch": ["^nginx-deployment\\.yaml$"],
        "matchStrings": [
          "image: nginx:(?<currentValue>\\d+\\.\\d+\\.\\d+)"
        ],
        "datasourceTemplate": "docker",
        "depNameTemplate": "nginx",
        "versioningTemplate": "semver"
      }
    ]
}
  